<Window x:Class="Bank.Bank_Application"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bank"
        mc:Ignorable="d"
        Title="Bank_Application" Height="150" Width="500">
    <Grid>
        <Button Content="Log out" HorizontalAlignment="Left" Margin="401,20,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <ComboBox x:Name="Action" HorizontalAlignment="Left" Margin="21,19,0,0" VerticalAlignment="Top" Width="140">
            <ComboBoxItem Content="Chose action" IsSelected="True"></ComboBoxItem>
            <ComboBoxItem Content="Create new account"></ComboBoxItem>
            <ComboBoxItem Content="Work with exsisting account"></ComboBoxItem>
            <ComboBoxItem Content="Get sheets" > </ComboBoxItem>
        </ComboBox>
        <Button Content="Continue" HorizontalAlignment="Left" Margin="179,21,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

    </Grid>
</Window>
